FDL Massestrom_Eq1 {
  EQUATION m^^{.} = (m / l_{"a"}) * v + c * a
  WITH:
    ALIASES:
      m^^{.} = "Massestrom";
      l_{"a"} = "Gutabstand";
      m = "Durchschnittliche Masse";
      v = "Fördergeschwindigkeit";
      c = "Test Skalar";
      a = "Test Parameter";

    SPECIFICATIONS:
      m^^{.} = "mass"/"time";
      m = "mass";
      l_{"a"} = "length";
      v = "length"/"time";

    REFERENCES:
      m^^{.} = 20-40 kg/s IN INT; //I=Integer=ganze Zahl
      m = 1-5 kg IN FLOAT[2]; //F=Float=fließkomma Zahl + 2 Nachkomma
      l_{"a"} = 5-100 m IN FLOAT;
      v = 1 km/h;
      c = -5 - -1 IN INT;
      a = PARAMETER IN INT;
}